{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "DomainName"
  ],
  "properties": {
    "DomainName": {
      "type": "string"
    },
    "SubjectAlternativeNames": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "IdempotencyToken": {
      "type": "string"
    },
    "DomainValidationOptions": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "minItems": 1,
      "uniqueItems": true
    }
  }
}
